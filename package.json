        "lodash": "^4.17.4",
        "popper.js": "^1.14.3",
        "resolve-url-loader": "^3.1.0",
        "sass": "^1.26.10",
        "sass-loader": "^8.0.2",
        "vue": "^2.5.7",
        "vue-template-compiler": "^2.6.11"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^5.8.1",
        "chart.js": "^2.9.3",
        "chartjs-plugin-annotation": "^0.5.7",
        "gulp": "^4.0.2",
